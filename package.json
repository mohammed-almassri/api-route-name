{
    "name": "api-route-name",
    "version": "2.1.0",
    "description": "give names to your api endpoints and easily get their url.",
    "main": "dist/index.js",
    "repository": "github:mohammed-almassri/api-route-name",
    "scripts": {
        "build": "babel src/ -d dist/",
        "pretest": "npm run build",
        "test": "mocha || true",
        "lint": "eslint --max-warnings 99999 --ignore-path .gitignore . --fix",
        "precommit-msg": "echo '\\e[33mPre-commit checks...' && exit 0",
        "postcommit-msg": "echo '\\e[32m\n\n\nSuccess :)\\e[39m'"
    },
    "keywords": [
        "routing",
        "named routes",
        "api"
    ],
    "author": "mohammed almassri",
    "license": "MIT",
    "dependencies": {
        "qs": "^6.9.0",
        "url-pattern": "^1.0.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "chai": "^4.2.0",
        "eslint": "^6.6.0",
        "mocha": "^10.2.0",
        "nodemod": "^1.5.19",
        "pre-commit": "^1.2.2"
    },
    "pre-commit": [
        "precommit-msg",
        "lint",
        "postcommit-msg"
    ]
}
